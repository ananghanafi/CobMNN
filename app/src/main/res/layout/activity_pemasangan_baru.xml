                    <android.support.design.widget.TextInputLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <EditText
                            android:id="@+id/namaBaru"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="top|left"
                            android:hint="Nama"
                            android:scrollbars="vertical" />
                    </android.support.design.widget.TextInputLayout>

                    <!--<TextView-->
