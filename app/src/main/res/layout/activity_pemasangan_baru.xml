    <LinearLayout
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="vertical">

        <GridLayout
            android:id="@+id/grid"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:columnCount="2">

            <Button
                android:id="@+id/btCekBiaya"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_columnWeight="1"
                android:layout_gravity="fill_horizontal"
                android:background="@color/colorPrimaryDark"
                android:onClick="btCekBiaya"
                android:text="Cek Biaya" />

            <Button
                android:id="@+id/uploadGmail"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_columnWeight="1"
                android:layout_gravity="fill_horizontal"
                android:background="@color/colorAccent"
                android:onClick="btUploadFile"
                android:text="Kirim file dengan Gmail" />

        </GridLayout>

        <Button
            android:id="@+id/btPesanBaru"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:background="@color/colorPrimary"
            android:gravity="center"
            android:onClick="btPesanPasangBaru"
            android:text="Pesan" />

    </LinearLayout>


